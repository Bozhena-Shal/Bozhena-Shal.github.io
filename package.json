{
  "name": "tailwind-css-starter-postcss",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "postcss ./src/styles.css -o ./public/dist/styles.css",
    "watch": "onchange 'tailwind.config.js' './public/**/*.html' './src/**/*.css' -- npm run build",
    "prod": "NODE_ENV=production postcss ./src/styles.css -o ./public/dist/styles.css",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public --no-cache"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.2.1",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2",
    "vue-confetti": "^2.3.0"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "onchange": "^7.1.0"
  }
}
